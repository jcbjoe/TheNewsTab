{
  "name": "The News Tab",
  "description": "A new tab extension for chrome that allows you to view the latest news",
  "version": "1.0",
  "chrome_url_overrides": { "newtab": "index.html" },
  "manifest_version": 2,
  "permissions": ["topSites", "<all_urls>", "storage"],
  "background": {
    "scripts": ["rss-parser.min.js", "background.js"],
    "persistent": false
  }
}
